DESCRIPTION:
Modelo estructural descrito por Haeseker 2016

[LONGITUDINAL]
input = {V, Cl}

EQUATION:
; PK model definition
Cc = pkmodel(V, Cl)

OUTPUT:
output = {Cc}

[POPULATION]
input = {mCl, gCl, mV, gV}

DEFINITION:
Cl = {distribution = lognormal, mean = mCl, variance = gCl }
V  = {distribution = lognormal, mean = mV,  variance = gV }

